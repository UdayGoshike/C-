#include <iostream>
using namespace std;

int main() {
    int n; 
    cout<<"enter size of the array:";
    cin>>n;
    int arr[n];
    n=sizeof(arr)/sizeof(int);
    cout<<"enter elements of array:";
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    for(int i=1 ;i<n;i++){
        int curr=arr[i];
        for(int j=i-1;j>=0;j--){
            if(curr<arr[j]){
                swap(arr[j],arr[j+1]);
            }
        }
    }
    cout<<"sorted array:";
    for(int i=0;i<n;i++){
        cout<<arr[i]<<",";
    }
}
