#include<iostream>
#include <algorithm>
using namespace std;
int main(){
  int arr[8]={5,52,53,6,3,8,9,10};
  cout<<"ascending order"<<endl;
  sort(arr,arr+8);
  for(int i=0;i<8;i++){
    cout<<arr[i]<<",";
  }
  cout<<endl;
  cout<<"descendng order"<<endl;
  sort(arr,arr+8,greater<int>());
  for(int i=0;i<8;i++){
    cout<<arr[i]<<",";
  }
}
