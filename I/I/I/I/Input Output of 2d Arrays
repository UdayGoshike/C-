#include <iostream>
using namespace std;

int main() {
    int student[3][2]; // 2D array to store student data (3 rows, 2 columns)

    // Input student data
    cout << "Enter the values for the 3x2 array (3 rows and 2 columns):" << endl;
    for(int i = 0; i < 3; i++) {
        for(int j = 0; j < 2; j++) {
            cin >> student[i][j]; // Input each element
        }
    }

    // Output the student data
    cout << "Array elements are:" << endl;
    for(int i = 0; i < 3; i++) {
        for(int j = 0; j < 2; j++) {
            cout << student[i][j] << " "; // Print each element with space
        }
        cout << endl; // Move to the next line after each row
    }

    return 0;
}
