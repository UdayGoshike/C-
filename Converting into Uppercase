#include <iostream>
#include<cstring>
using namespace std;

int main() {
    char arr[]="ApPlEs";
    int n=strlen(arr);
    int word;
    for(int i=0;i<n;i++){
        word=arr[i];
        if(word>=97&&word<=127){
            arr[i]=arr[i]-32;
            word=0;
        }
    }
    cout<<arr<<endl;
    return 0;
}
