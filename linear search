#include <iostream>
using namespace std;

int main() { 
  int target;
  cin>>target;
  int arr[5]={1,2,3,4,5};
  int n=sizeof(arr)/sizeof(int);
  for(int i=0;i<n;i++){
    if (arr[i]==target){
      cout<<"found at index:"<<i;
      break;
    }
  }
  if(arr[n-1]!=target){
    cout<<"not found";
  }
}
